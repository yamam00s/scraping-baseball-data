{
    "extends":  [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 2]
    },
    "globals": {
        "document": true,
        "window": true,
        "location": true
    }
}
